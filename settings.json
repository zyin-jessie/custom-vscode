{
    "files.autoSave": "afterDelay",
    "workbench.colorCustomizations": {
        "statusBar.background": "#3877FF",
        "statusBar.foreground": "#FFFFFF"
    }, 
    "window.title": "${dirty}${activeEditorShort}",
    "explorer.confirmDelete": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "hediet.vscode-drawio.resizeImages": null,
    "codesnap.transparentBackground": true,
    "codesnap.target": "window",
    "redhat.telemetry.enabled": true,
    "codesnap.showWindowTitle": true,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "workbench.colorTheme": "One Dark Pro",
    "vscode-pets.petSize": "small",
    "vscode-pets.theme": "forest",
    "vscode-pets.throwBallWithMouse": true,
    "editor.smoothScrolling": true,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "explorer.compactFolders": false,
    "discord.removeRemoteRepository": true,
    "discord.detailsIdling": "nagalulu",
    "discord.lowerDetailsEditing": "1v9 machine",
    "php.validate.executablePath": "C:/xampp/php/php.exe",
    "liveServer.settings.AdvanceCustomBrowserCmdLine": "chrome",
    "liveServer.settings.ChromeDebuggingAttachment": false,
    "editor.cursorSmoothCaretAnimation": "on",
    "json.schemas": [

    ],
    "editor.minimap.renderCharacters": false,
    "javascript.referencesCodeLens.enabled": true,
    "typescript.referencesCodeLens.enabled": true,
    "screencastMode.keyboardOverlayTimeout": 500,
    "screencastMode.verticalOffset": 0,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.iconTheme": "material-icon-theme",

    // "editor.copyWithSyntaxHighlighting": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.emptySelectionClipboard": false,
    "workbench.editor.enablePreview": false,
    "window.newWindowDimensions": "inherit",
    "editor.multiCursorModifier": "ctrlCmd",
    "files.trimTrailingWhitespace": true,
    "diffEditor.renderSideBySide": false,
    "editor.snippetSuggestions": "top",
    "editor.detectIndentation": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    // "workbench.editor.showTabs": "none",
    // "editor.lineNumbers": "off",
    // "editor.guides.indentation": false,

    "breadcrumbs.enabled": false,
    "scm.diffDecorations": "none",
    "editor.hover.delay": 1500,
    "editor.hover.enabled": true,
    "editor.matchBrackets": "never",
    "workbench.tips.enabled": false,
    "editor.colorDecorators": false,
    "git.decorations.enabled": false,  // Git Decoration
    "workbench.startupEditor": "none",
    "editor.lightbulb.enabled": "off",
    "editor.selectionHighlight": false,
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlight": "none",
    "editor.occurrencesHighlight": "off",
    "problems.decorations.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",

    "vscode_custom_css.imports": [
        "file:///C:/your-path/custom-vscode.css",
        "file:///C:/your-path/custom-vscode.js",
    ],
    "editor.minimap.enabled": false,
    "window.customTitleBarVisibility": "windowed",
    "workbench.activityBar.location": "hidden",
    "workbench.statusBar.visible": false,
    "editor.stickyScroll.enabled": false,
    "workbench.editor.editorActionsLocation": "hidden",
    "chat.commandCenter.enabled": false,
    "workbench.navigationControl.enabled": false,
    "window.commandCenter": false,
}
